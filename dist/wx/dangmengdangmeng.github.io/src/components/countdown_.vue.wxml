<template name="f911e460">
  <view class="_div data-v-3fed74ac countdown-container pt90">
    <view class="_div data-v-3fed74ac countdown-box flex" wx:if="{{count_down!='活动结束'}}">
      <view class="_div data-v-3fed74ac dot-badge">·</view>距投票结束： <view class="_div data-v-3fed74ac countdown-text">{{count_down[0]}}</view>天 <view class="_div data-v-3fed74ac countdown-text">{{count_down[1]}}</view>时 <view class="_div data-v-3fed74ac countdown-text">{{count_down[2]}}</view>分
    </view>
    <view class="_div data-v-3fed74ac countdown-box flex" style="color:red;" wx:if="{{count_down=='活动结束'}}">投票已结束</view>
    <view class="_div data-v-3fed74ac total-count-box" wx:if="{{count_down_info&&count_down_info.show_info}}">
      <view class="_div data-v-3fed74ac total-count-item">
        <view class="_p data-v-3fed74ac">参赛萌宝</view>
        <view class="_p data-v-3fed74ac total-count">{{count_info[0]}}</view>
      </view>
      <view class="_div data-v-3fed74ac total-count-item">
        <view class="_p data-v-3fed74ac">累计投票</view>
        <view class="_p data-v-3fed74ac total-count">{{count_info[1]}}</view>
      </view>
      <view class="_div data-v-3fed74ac total-count-item">
        <view class="_p data-v-3fed74ac">神助攻</view>
        <view class="_p data-v-3fed74ac total-count">{{count_info[2]}}</view>
      </view>
    </view>
  </view>
</template>